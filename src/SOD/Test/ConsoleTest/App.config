		<add key="DataLogFile" value="Log.txt" />
	</appSettings>
	<connectionStrings>
		<!--<add name="local" connectionString="Data Source=.;Initial Catalog=LocalDB;Integrated Security=True" providerName="SqlServer" />-->
		<add name="local"
		     connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=~/Database1.mdf;Integrated Security=True"
		     providerName="SqlServer" />
	</connectionStrings>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
</configuration>